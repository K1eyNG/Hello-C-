#include <stdio.h>

int main()
{
    int a;
    scanf("%d",&a);
    if(a==6){
        a=1;
    }
    else if(a==7){
        a=2;
    }
    else{
        a=a+2;
    }
    switch(a){
        case 1:
            printf("%d",a);
            break;
        case 2:
            printf("%d",a);
            break;
        case 3:
            printf("%d",a);
            break;
        case 4:
            printf("%d",a);
            break;
        case 5:
           printf("%d",a);
            break;
        case 6:
            printf("%d",a);
            break;
        case 7:
            printf("%d",a);
            break;
    }
}
